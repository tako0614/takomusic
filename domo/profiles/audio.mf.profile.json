{
  "tako": {
    "profileVersion": 1
  },
  "profileName": "domo_demo_audio",
  "renderer": "audio.simple",
  "output": {
    "path": "out/daisyou.mp3",
    "wavPath": "out/daisyou.wav",
    "sampleRate": 44100,
    "bitrate": 192,
    "neutrinoRoot": "C:/Users/shout/NEUTRINO",
    "neutrinoModel": "MERROW",
    "neutrinoThreads": 4,
    "neutrinoTranspose": 0,
    "neutrinoGain": 0.8
  },
  "bindings": [
    {
      "selector": { "sound": "guitar" },
      "config": { "preset": "guitar", "gain": 0.45, "pan": 0.15 }
    },
    {
      "selector": { "sound": "bass" },
      "config": { "preset": "bass", "gain": 0.55, "pan": 0 }
    },
    {
      "selector": { "sound": "kit_standard" },
      "config": { "preset": "drums", "gain": 0.9, "pan": 0 }
    },
    {
      "selector": { "sound": "lead_vocal" },
      "config": { "preset": "vocal", "gain": 0.6, "pan": 0, "vibratoDepth": 0.01, "vibratoRate": 5.2 }
    }
  ]
}
